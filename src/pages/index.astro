---
import MainLayout from "../layouts/MainLayout.astro";
import Link from "../components/Link.astro";
import { Image } from "astro:assets";
import thumbsup from "../assets/images/thumbsup.png";
import Heading from "../components/Heading.astro";
import Text from "../components/Text.astro";
import Button from "../components/Button.astro";
---

<MainLayout title="Titel">
  <main class="container mx-auto p-4">
    <Heading id="top" htmlheading="h1" size="large" font="display" color="red">Lav noget flot!</Heading>
    <Text teksttype="brødtekst" weight="bold">Så bliver Anders glad:-)</Text>
    <Link href="https://google.com" target="_blank" >Gå til Astro</link>
      <Button href="#top">Share your instant</Button>
  </main>
</MainLayout>
